  <body>
    <!--header-->
    <div class="main-header">
      <header class="header">
        <div class="container">
          <div class="row">
            <div class="goal-summary pull-left" style="padding-left: 0px;">
              <div
                style="
                  background-color: white;
                  border-radius: 30px 30px 30px 30px;
                "
              >
                <div style="width: 160px" class="backers">
                  <img
                    src="img/logo_web.png"
                    alt="로고"
                    c
                    width="160"
                    height="130"
                  />
                </div>
                <div class="backers">
                  <a href="index.njk">
                    <img
                      src="img/title.png"
                      alt="로고"
                      style="display: block"
                    />
                  </a>
                  <!-- <h3 style="font-size: 50px; font-weight: bold;" >너 또 돈 써?</h3> -->
                </div>
              </div>
            </div>
          </div>
        </div>
      </header>
    </div>
    <!--/header-->
    <!--main content-->
    <div class="main-content">
      <div class="container">
        <div class="row">
          <div class="content col-md-8 col-sm-12 col-xs-12">
            <!--달력화면-->
            <div class="section-block">
              <div class="main_calender">
                <head>
                  <meta charset="UTF-8" />
                  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
                  <meta
                    name="viewport"
                    content="width=device-width, initial-scale=1.0"
                  />
                  <title>Challenge page</title>
                  <!-- <link rel ="stylesheet" href="css/style_calender.css"> -->
                </head>
                <body>
                  <div class="calendar">
                    <div class="header">
                      <div class="year-month"></div>
                      <div class="nav">
                        <button
                          class="nav-btn go-prev"
                          onclick="preMonth(event)"
                        >
                          &lt;
                        </button>
                        <button
                          class="nav-btn go-today"
                          onclick="goToday(event)"
                        >
                          Today
                        </button>
                        <button
                          class="nav-btn go-next"
                          onclick="nextMonth(event)"
                        >
                          &gt;
                        </button>
                      </div>
                    </div>
                    <div class="main">
                      <div class="days">
                        <div class="day">일</div>
                        <div class="day">월</div>
                        <div class="day">화</div>
                        <div class="day">수</div>
                        <div class="day">목</div>
                        <div class="day">금</div>
                        <div class="day">토</div>
                      </div>
                      <div class="dates" onclick="handleDateClick()"></div>
                    </div>
                  </div>
                </body>
              </div>
            </div>
          </div>
          <!--/tabs-->
          <!--/main content-->
          <!--sidebar-->
          <div class="content col-md-4 col-sm-12 col-xs-12">
            <div class="section-block summary">
              <h1 class="section-title">{{ nickname }}</h1>
              <div class="profile-contents">
                <img
                  src="{{ user_image }}"
                  class="profile-image img responsive"
                  alt="로그인 해주세요"
                />
                {% if loggedIn %}
                <h2 class="user-ranking">{{ranking}}</h2>
                <a href="/auth/logout"> 로그아웃 하기</a>
                <p></p>
                <a
                  href="/auth/unlink"
                  class="btn_unlink"
                  onclick="return confirmUnlink();"
                  >회원 탈퇴</a
                >

                <script>
                  function confirmUnlink() {
                    return confirm("회원 탈퇴를 진행하시겠습니까?");
                  }
                </script>

                {% endif %} {% if not loggedIn %}
                <a href="/auth/kakao" class="btn btn-contact">
                  <img
                    src="img/kakao_login_medium_narrow.png"
                    alt="Kakao Login"
                  />
                </a>
                {% endif %}
              </div>
            </div>
            <div class="section-block">
              <!-- <h1 class="section-title">REWARDS</h1> -->
              <!--reward blocks-->
              <div class="mypage-block">
                <!-- <span><i class="fa fa-users"></i> 180 backers</span> -->
                <a href="/mypage" class="btn btn-mypage">MY PAGE</a>
              </div>
            </div>
            <div class="section-block">
              <div class="mypage-block">
                <!-- 버튼이 mypage 버튼과 같은 디자인이라 그냥 btn-mypage라고 똑같이 썼음 -->
                <a href="http://localhost:8080/" class="btn btn-mypage"
                  >MAIN PAGE</a
                >
              </div>
            </div>
          </div>
          <!--/sidebar-->
        </div>
      </div>
    </div>
    <footer class="footer">
      <div class="container">
        <div class="row">
          <span class="copyright">Created by Web-gurae</span>
        </div>
      </div>
    </footer>

    <script src="js/calender.js"></script>
  </body>
</html>
